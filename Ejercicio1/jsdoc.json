{
    "source": {
        "include":["./src"],
        "indlucePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts":{
        "recurse": true,
        "destination": "./docs/"
    }
}